pipeline{
      stages{
            stage('Build'){
                  steps{
                        //sh 'mvn --version'
                        sh 'node --version'
                        echo "building an application"
                  }
            }
            stage('Test'){
                  steps{
                        echo "Testing an application"
                  }
            }
            stage('Deploy'){
                  steps{
                        echo "Deploying an application"
                  }
            }
            

      }

}
      


